{
    "name": "searchSavior",
    "description": "automated search saver",
    "version": "0.1.0",
    "manifest_version": 2,
    "icons":{
        "19": "./extension-icon.png"
    },
    "background": {
        "scripts": ["./background.js"]
        
    },
    "options_page": "./projectpage.html",
    "browser_action": {
        "default_popup": "popup.html"
    },
    "permissions": [
        "tabs",
        "https://www.google.com/",
        "storage"
    ]
}